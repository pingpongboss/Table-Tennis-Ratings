<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	style="@style/ActionBar">
	<Button style="@style/ActionBarItem.Logo" android:id="@+id/logo"></Button>
	<FrameLayout style="@style/ActionBarItem.ProviderLogoLayout">
		<Button android:layout_height="wrap_content"
			android:layout_width="wrap_content" style="@style/ActionBarItem.ProviderLogo"
			android:id="@+id/provider_logo"></Button>
	</FrameLayout>
	<TextView android:id="@+id/title" android:text="Table Tennis Ratings"
		style="@style/ActionBarItem.Text"></TextView>
</LinearLayout>
