<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	style="@style/ActionBar">
	<Button style="@style/ActionBarItem.Logo" android:id="@+id/logo"></Button>
	<FrameLayout style="@style/ActionBarItem.ProviderLogoLayout">
		<ImageView android:layout_height="wrap_content"
			android:layout_width="wrap_content" style="@style/ActionBarItem.ProviderLogo"
			android:id="@+id/provider_logo"></ImageView>
	</FrameLayout>
	<TextView android:id="@+id/title" style="@style/ActionBarItem.Text" android:text="pingpongboss"></TextView>
</LinearLayout>
