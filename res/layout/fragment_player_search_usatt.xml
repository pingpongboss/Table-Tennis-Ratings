<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	style="@style/RootLayout" android:orientation="vertical">
	<include android:layout_height="25dp" android:layout_width="fill_parent"
		layout="@layout/action_bar" android:id="@+id/action_bar"></include>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:orientation="horizontal"
		android:id="@+id/searchLinearLayout">
		<EditText android:hint="Last Name" android:id="@+id/usattPlayerNameEditText" style="@style/EditText.Left.Search"></EditText>
		<ImageButton android:id="@+id/searchButton" style="@style/Button.Right.Search"></ImageButton>
	</LinearLayout>
	<ListView style="@style/TransparentListView" android:id="@id/android:list"></ListView>
	<TextView android:id="@id/android:empty" android:text="No History"
		android:layout_height="fill_parent" android:layout_width="fill_parent"
		android:gravity="center"></TextView>
</LinearLayout>
